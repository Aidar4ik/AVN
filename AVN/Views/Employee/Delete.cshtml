@model AVN.Model.Entities.Employee

<div class="container">
    <h1>Подтверждение удаления</h1>
    <p>Вы уверены, что хотите удалить этого преподавателя?</p>
    <div class="card">
        <div class="card-body">
            <p class="card-text">@Model.SName</p>
            <p class="card-text">@Model.Name</p>
            <p class="card-text">@Model.PName</p>
            <form method="post" action="/Employee/DeleteConfirmed/@Model.Id">
                <button type="submit" class="btn btn-danger">Удалить</button>
                <a href="/Employee/Index" class="btn btn-secondary">Отмена</a>
            </form>
        </div>
    </div>
</div>